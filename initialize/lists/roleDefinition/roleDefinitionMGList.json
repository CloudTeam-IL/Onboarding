{
    "$schema": "https://schema.management.azure.com/schemas/2019-08-01/managementGroupDeploymentTemplate.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
    },
    "variables": {
        "roleDefList": [
            {
                "roleName": "Cost Management Export",
                "type": "customRule",
                "description": "Can view, create and update cost management exports",
                "permissions": [
                    {
                        "actions": [
                            "Microsoft.CostManagement/register/action",
                            "Microsoft.CostManagementExports/register/action",
                            "Microsoft.Management/managementGroups/read",
                            "Microsoft.Resources/subscriptions/read",
                            "Microsoft.CostManagement/reports/read",
                            "Microsoft.CostManagement/reports/action",
                            "Microsoft.CostManagement/exports/action",
                            "Microsoft.CostManagement/exports/read",
                            "Microsoft.CostManagement/exports/write",
                            "Microsoft.CostManagement/exports/run/action"
                        ]
                        // "notActions": [],
                        // "dataActions": [],
                        // "notDataActions": []
                    }
                ]
            }
        ]
    },
    "resources": [],
    "outputs": {
        "roleDefList": {
            "type": "array",
            "value": "[variables('roleDefList')]"
        }
    }
}